[project]
title = DefVideoAds
version = 1.0
dependencies = https://github.com/andsve/dirtylarry/archive/master.zip

[bootstrap]
main_collection = /example/main.collectionc

[input]
game_binding = /input/game.input_bindingc

[display]
width = 1280
height = 720

[physics]
scale = 0.02

[script]
shared_state = 1

[library]
include_dirs = defunityads

[ios]
bundle_identifier = com.agulev.test

[android]
package = com.agulev.test
immersive_mode = 1

